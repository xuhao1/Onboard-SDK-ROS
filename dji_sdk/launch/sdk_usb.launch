<launch>
	<node pkg="dji_sdk" type="dji_sdk_node" name="dji_sdk" output="screen">
		<!-- node parameters -->
		<param name="serial_name" type="string" value="/dev/ttyUSB0"/>
		<param name="baud_rate" type="int" value="921600"/>
		<param name="app_id" type="int" value="10086"/>
		<param name="app_version" type="int" value="1"/>
		<param name="app_bundle_id" type="string" value="Welcome to use dji-sdk"/>
		<param name="enc_key" type="string" value="12345678901234567890123456789012"/>
		<param name="groundstation_enable" type="int" value="1"/>
		<param name="uart_or_usb" type="int" value="0"/>
		<param name="A3_or_M100" type="int" value="0"/>
	</node>
</launch>
